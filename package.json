{
  "name": "@landas/aa-userop",
  "version": "0.0.1",
  "description": "A library to build User Operations in a simple and easy way.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "./dist/index.d.ts",
  "repository": {
    "url": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify --sourcemap",
    "yalc-publish": "yarn build && yalc push",
    "dev": "npx tsc && nodemon src/index.ts",
    "lint": "eslint 'src/**/*.ts'"
  },
  "keywords": [
    "Account Abstraction",
    "ERC-4337",
    "Ethereum",
    "User Operation"
  ],
  "author": "@Lando (Francisco Landino)",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.11.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.6.0"
  },
  "dependencies": {
    "axios": "1.7.4",
    "viem": "2.14.2",
    "yarn": "1.22.22"
  },
  "include": [
    "src/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
